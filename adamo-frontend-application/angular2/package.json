    "@types/jasmine": "3.3.9",
    "@types/jquery": "^3.3.29",
    "@types/jszip": "^3.1.4",
    "@types/node": "^11.9.4",
    "@types/redux-logger": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "@types/yargs": "12.0.9",
    "codelyzer": "~4.5.0",
    "electron": "^5.0.6",
    "electron-packager": "^14.0.1",
    "jasmine-core": "~3.3.0",
