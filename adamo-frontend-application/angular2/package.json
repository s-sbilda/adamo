{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "serve": "ng serve --host 0.0.0.0 --port 8085",
    "serve-dev": "ng serve --environment=dev --host 0.0.0.0 --port 8085",
    "config": "ts-node ./src/set-env.ts",
    "start": "npm run config -- --environment=dev && npm run serve-dev",
    "build": "npm run config -- --environment=dev && ng build --environment=dev",
    "docker:build": "ng build --environment=dev",
    "config-prod": "node -r ./../.env",
    "config-dev": "node -r ./../.env.dev",
    "start-full-prod": "npm-run-all -p config-prod start-mqtt-server-prod start-node-backend-prod start",
    "start-full-dev": "npm-run-all -p config-dev start-mqtt-server-dev start-node-backend-dev start",
    "start-mqtt-server-prod": "node ../API/lib/mqtt/mqttserver.js",
    "start-mqtt-server-dev": "node -r dotenv/config ../API/lib/mqtt/mqttserver.js dotenv_config_path=./../.env.dev",
    "start-node-backend-prod": "node ../API/lib/express/express.js",
    "start-node-backend-dev": "node  -r dotenv/config ../API/lib/express/express.js dotenv_config_path=./../.env.dev",
    "test": "karma start",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "clean": "rm -rf node_modules package-lock.json yarn.lock",
    "clean:install": "npm run clean && npm install",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds --darwinDarkModeSupport=true",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/Electron\\ tutorial\\ app-darwin-x64/Electron\\ tutorial\\ app.app electron-tutorial-app --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-debian-installer": "electron-installer-debian --src release-builds/electron-tutorial-app-linux-x64/ --arch amd64 --config debian.json",
    "compodoc": "npx compodoc -p src/tsconfig.app.json"
  },
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/build-angular": "^0.13.4",
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/material": "^7.2.2",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@material/dialog": "^3.2.0",
    "angular-resizable-element": "^3.2.4",
    "angular-split": "^3.0.2",
    "array-unique": "^0.3.2",
    "big-integer": "^1.6.34",
    "bootstrap": "^4.3.1",
    "bpmn-js": "^3.2.1",
    "bpmn-js-properties-panel": "^0.29.0",
    "buffer": "^5.2.1",
    "bulma": "^0.7.5",
    "camunda-bpmn-moddle": "^3.2.0",
    "core-js": "^2.4.1",
    "diagram-js-origin": "^1.1.1",
    "dotenv": "^6.2.0",
    "es6-promise": "^4.2.6",
    "es6-shim": "^0.35.4",
    "express": "^4.16.2",
    "file-saver": "^2.0.1",
    "hammerjs": "^2.0.8",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.2.1",
    "js-interpreter": "^1.4.6",
    "jszip": "^3.1.5",
    "moment": "^2.24.0",
    "mqtt": "^2.16.0",
    "ng2-bs3-modal": "0.15.0",
    "ng2-search-filter": "^0.4.7",
    "ngx-logger": "^3.3.11",
    "ngx-mqtt": "^6.8.5",
    "redux": "^4.0.1",
